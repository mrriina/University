<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.TextField?>
<VBox alignment="CENTER" prefHeight="202.0" prefWidth="354.0" spacing="20.0"
      xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.spp_lab3.SecondaryController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <children>
        <ComboBox fx:id="productCombo" prefHeight="26.0" prefWidth="402.0" promptText="Select product" visibleRowCount="3" />
        <ComboBox fx:id="warehouseCombo" prefHeight="26.0" prefWidth="402.0" promptText="Select warehouse" visibleRowCount="3" />
        <TextField fx:id="idAmount" layoutX="168.0" layoutY="26.0" maxHeight="-Infinity" prefHeight="20.0" prefWidth="190.0"/>
        <HBox prefHeight="26.0" prefWidth="199.0">
            <children>
                <Button fx:id="submitButton" alignment="CENTER" mnemonicParsing="false" prefHeight="26.0" prefWidth="402.0" text="Apply" />
            </children>
        </HBox>
    </children>
</VBox>