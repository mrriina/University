<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>

<?import javafx.scene.control.TextField?>
<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="com.example.spp_lab5_client.GameController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label fx:id="result" />

    <Label fx:id="command">
        <padding><Insets top="10" bottom="10" /></padding>
    </Label>

    <HBox>
        <TextField fx:id="numberInput" />
        <Button fx:id="enterButton" text="Ввод" onAction="#onClick" disable="true" />
    </HBox>
</VBox>
